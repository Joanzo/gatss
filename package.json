{
  "name": "gatsby-ts-sb-starter",
  "private": true,
  "description": "A starter with Gatsby Typescript Storybook with Airbnb Linting setup",
  "version": "0.1.0",
  "author": "Joe Un <jo@joanzo.com>",
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.19.32",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.31",
    "gatsby-plugin-sharp": "^2.4.3",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typescript": "^2.2.1",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
