    "babel-preset-gatsby": "^0.2.35",
    "babel-preset-react-app": "^9.1.1",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-docgen-typescript-loader": "^3.7.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "react": "^16.13.0"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "typescript",
    "jest",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "concurrently --kill-others \"npm run develop\" \"npm run storybook\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "SET NODE_ENV=production&& start-storybook -p 6006 -c .storybook",
    "build-storybook": "SET NODE_ENV=production&& build-storybook -s static"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joanzo/gatsby-ts-sb-jest-starter"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
